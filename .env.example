# 服务配置
SERVER_PORT=8080
SERVER_HOST=0.0.0.0

# Genkit 配置
GENKIT_API_KEY=your-api-key-here
GENKIT_MODEL=gemini-2.5-flash
GENKIT_DEFAULT_TEMPERATURE=0.7
GENKIT_DEFAULT_MAX_TOKENS=2000

# PostgreSQL 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your-password-here
DB_NAME=genkit_ai_service
DB_SSLMODE=disable
DB_MAX_OPEN_CONNS=25
DB_MAX_IDLE_CONNS=5
DB_CONN_MAX_LIFETIME=5m
# GORM 日志级别: silent, error, warn, info
DB_LOG_LEVEL=warn

# 日志配置
LOG_LEVEL=info
LOG_FORMAT=json

# 会话配置
SESSION_TIMEOUT=30m
SESSION_CLEANUP_INTERVAL=5m
